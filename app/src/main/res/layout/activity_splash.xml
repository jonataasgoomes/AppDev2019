<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorSplash"
    tools:context=".activity.SplashActivity" >

    <ImageView
        android:id="@+id/imageSplash"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="0dp"
        app:srcCompat="@mipmap/meau_malha" />

    <ImageView
        android:id="@+id/imageSplashLogo"
        android:layout_width="276dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/imageSplash"
        android:layout_alignEnd="@+id/imageSplash"
        android:layout_alignBottom="@+id/imageSplash"
        android:layout_alignParentStart="true"
        android:layout_marginStart="66dp"
        android:layout_marginTop="251dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="251dp"
        app:srcCompat="@mipmap/meau1" />
</RelativeLayout>